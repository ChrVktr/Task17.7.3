per_cent={'ТКБ': 5.6, 'СКБ': 5.9, 'ВТБ': 4.28, 'СБЕР': 4.0}
a= per_cent['ТКБ']
b= per_cent['СКБ']
c= per_cent['ВТБ']
d= per_cent['СБЕР']
money= float(input("Сумма вклада: "))
money= float(input("Сумма вклада: "))
deposit_a=a*money/10
deposit_b=b*money/10
deposit_c=c*money/10
deposit_d=d*money/10
deposits=(deposit_a,deposit_b,deposit_c,deposit_d)
print(deposits)
print("Максимальная сумма, которую вы можете заработать — [%i]" % max(deposits))
